quicksort :=
  ([]) => []
  ([pivot, ...rest]) =>
    l := quicksort([n | n in rest if n <= pivot])
    r := quicksort([n | n in rest if n > pivot])
    l + [pivot] + r

list := quicksort([5, 7, 2, 0, 4, 3])
print(list[0].to_str)
print(list[1].to_str)
print(list[2].to_str)
print(list[3].to_str)
print(list[4].to_str)
print(list[5].to_str)
