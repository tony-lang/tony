print := eval('console.log') as String -> Void
to_str := eval('parseInt') as Number -> String

! := (a :: Number) => eval('-{a.to_str}') as Number
+ := (a :: Number, b :: Number) => eval('{a.to_str} + {b.to_str}') as Number
- := (a :: Number, b :: Number) => eval('{a.to_str} - {b.to_str}') as Number
% := (a :: Number, b :: Number) => eval('{a.to_str} % {b.to_str}') as Number

++ := (a :: [Number], b :: [Number]) =>
  eval('[{a as String}].concat([{b as String}])') as [Number]

<= := (a :: Number, b :: Number) => eval('{a.to_str} <= {b.to_str}') as Boolean
< := (a :: Number, b :: Number) => eval('{a.to_str} < {b.to_str}') as Boolean
>= := (a :: Number, b :: Number) => eval('{a.to_str} >= {b.to_str}') as Boolean
> := (a :: Number, b :: Number) => eval('{a.to_str} > {b.to_str}') as Boolean

== := (a :: Number, b :: Number) => eval('{a.to_str} == {b.to_str}') as Boolean
