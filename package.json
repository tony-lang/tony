{
  "name": "tony-lang",
  "version": "0.1.4",
  "description": "The Tony programming language",
  "main": "dist/src/index.js",
  "types": "tony-lang.d.ts",
  "scripts": {
    "build": "babel . --ignore dist,node_modules --out-dir dist --extensions .ts",
    "start": "nodemon --watch src --watch test --exec 'yarn build' -e ts",
    "prettierlint": "prettier --check src test",
    "eslint": "eslint src test --ext .ts",
    "lint": "yarn prettierlint && yarn eslint",
    "prettier": "prettier --write src test",
    "fix": "yarn prettier && yarn eslint --fix",
    "test": "ava dist/test/index.test.js"
  },
  "engines": {
    "node": ">=12.0"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/tony-lang/tony.git"
  },
  "keywords": [
    "language"
  ],
  "author": "Jonas HÃ¼botter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tony-lang/tony/issues"
  },
  "homepage": "https://github.com/tony-lang/tony#readme",
  "dependencies": {
    "core-js": "^3.6.5",
    "deep-equal": "^2.0.3",
    "mkdirp": "^1.0.3",
    "outdent": "^0.7.1",
    "tree-sitter": "^0.17.0",
    "tree-sitter-tony": "0.2.0-alpha.19"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/deep-equal": "^1.0.1",
    "@types/mkdirp": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "ava": "^3.11.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "typescript": "^4.1.2"
  }
}
