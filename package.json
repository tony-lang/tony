  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/tony-lang/tony/issues"
  },
  "homepage": "https://github.com/tony-lang/tony#readme",
  "dependencies": {
    "commander": "^5.0.0",
    "core-js": "^3.6.4",
    "deep-equal": "^2.0.1",
    "inquirer": "^7.1.0",
    "mkdirp": "^1.0.3",
    "outdent": "^0.7.1",
    "rimraf": "^3.0.2",
    "tree-sitter": "^0.16.0",
